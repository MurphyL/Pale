---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">{% for post in site.posts %}
  <url>
    <loc>{{ post.url | prepend: site.baseurl | prepend: site.url  }}</loc>
    <lastmod>{{ post.date | date_to_long_string }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
  </url>{% endfor %}
</urlset>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">{% for post in site.posts %}
   <sitemap>
      <loc>{{ post.date | date_to_long_string }}</loc>
      <lastmod>{{ post.date | date_to_long_string }}</lastmod>
   </sitemap>{% endfor %}
</sitemapindex>
